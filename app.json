{
  "expo": {
    "entryPoint": "./src/Index.js",
    "name": "Bourd",
    "slug": "Bourd",
    "version": "1.0.0",
    "androidStatusBar": {"backgroundColor": "#121330","barStyle": "light-content"},
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121330"
    },
    "primaryColor": "#121330",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.devvie.bourd",
      "requireFullScreen": true,
      "supportsTablet": true
    },
    "android": {
      "package": "com.devvie.bourd",
      "adaptiveIcon": {
        "backgroundColor": "#121330"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
